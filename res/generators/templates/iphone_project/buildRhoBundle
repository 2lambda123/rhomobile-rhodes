#!/bin/sh


if [ "$RHO_BUNDLE_ALREADY_BUILDED" = "YES" ]; then

echo "RhoBundle already builded !"

elif [ "$ACTION" = "build" -o "$ACTION" = "" ]; then

source ~/.profile
cd ../..
rake switch_app
TARGET_RHODESLIBS_DIR=${SRCROOT}
cd <%= rhodes_root_path %>
rake build:iphone:rhobundle

RHO_BUNDLE_ALREADY_BUILDED="NO"

fi



