<?xml version = "1.0"?>
<?xml-stylesheet type="text/xsl" href="pb_help.xsl"?>

<PROPERTY_BAG>
	<METHODS>
		<METHOD name="getProperty" has_callback="optional">
			<DESC>This is documentation</DESC>
			<PARAMS>
				<PARAM name="propertyName" type="STRING">
					<DESC>The one property that I want to know about</DESC>
                </PARAM>
            </PARAMS>
            <RETURN>
                <DESC>The one property that I want to know about</DESC>
                <TYPE>STRING</TYPE>
            </RETURN>
        </METHOD>

        <METHOD name="getProperties" has_callback="optional">
            <DESC>This is documentation</DESC>
            <PARAMS>
                <PARAM name="arrayofNames" type="ARRAY">
                    <DESC>List of properties I want to know about</DESC>
                </PARAM>
            </PARAMS>
            <RETURN>
                <DESC>Map of properties I want to know about</DESC>
                <TYPE>HASH</TYPE>
            </RETURN>
        </METHOD>

        <METHOD name="getAllProperties" has_callback="optional">
            <DESC>This is documentation</DESC>
            <RETURN>
                <DESC>Map of properties I want to know about</DESC>
                <TYPE>HASH</TYPE>
            </RETURN>
        </METHOD>

        <METHOD name="setProperty">
            <DESC>This is documentation</DESC>
            <PARAMS>
                <PARAM name="propertyName" type="STRING">
                    <DESC>The one property name that I want to set</DESC>
                </PARAM>
                <PARAM name="propertyValue" type="STRING">
                    <DESC>The one property value that I want to set</DESC>
                </PARAM>
            </PARAMS>
        </METHOD>

        <METHOD name="setProperties">
            <DESC>This is documentation</DESC>
            <PARAMS>
                <PARAM name="propertyMap" type="HASH">
                    <DESC>Map of properties I want to set</DESC>
                </PARAM>
            </PARAMS>
        </METHOD>

        <METHOD name="clearAllProperties">
            <DESC>This is documentation</DESC>
        </METHOD>

    </METHODS>
</PROPERTY_BAG>
